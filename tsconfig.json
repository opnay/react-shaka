{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2015"]
  },
  "include": ["./src/**/*"]
}
